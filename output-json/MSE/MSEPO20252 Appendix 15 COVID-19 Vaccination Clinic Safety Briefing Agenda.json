            "data": {
                "is_heading": 0,
                "text": "This safety briefing must be held prior to the commencement of any COVID-19 vaccination clinic."
            },
            "type": "ListItem",
            "sequence_num": 5
        },
        {
            "data": {
                "is_heading": 0,
                "text": "All staff working in the vaccination centre must be present."
            },
            "type": "ListItem",
            "sequence_num": 6
        },
        {
            "data": {
                "is_heading": 0,
                "text": "Completion of the safety briefing must be documented on the COVID-19 Vaccine change checklist."
            },
            "type": "ListItem",
            "sequence_num": 7
        },
        {
            "type": "table",
            "sequence_num": 8,
            "data": {
                "num_rows": 10,
                "num_cols": 2,
                "table_items": [
                    {
                        "text": ""
                    },
                    {
                        "text": "Agenda Item"
                    },
                    {
                        "text": "1"
                    },
                    {
                        "text": "Confirm which vaccine being used for clinic"
                    },
                    {
                        "text": "2"
                    },
                    {
                        "text": "Confirm preparation requirements i.e. is dilution required?"
                    },
                    {
                        "text": "3"
                    },
                    {
                        "text": "Confirm final dose to be administered for vaccine being used"
                    },
                    {
                        "text": "4"
                    },
                    {
                        "text": "Confirm vaccine presentation in stock i.e. number of doses obtainable from each vial"
                    },
                    {
                        "text": "5"
                    },
                    {
                        "text": "Confirm post vaccination observation period for vaccine being used"
                    },
                    {
                        "text": "6"
                    },
                    {
                        "text": "Confirm location of anaphylaxis kits"
                    },
                    {
                        "text": "7"
                    },
                    {
                        "text": "Share any learning from incidents"
                    },
                    {
                        "text": "8"
                    },
                    {
                        "text": "Communicate any changes in National or Local policy"
                    },
                    {
                        "text": "9"
                    },
                    {
                        "text": "Do staff have any questions to ask or issues to raise?"
                    }
                ]
            }
