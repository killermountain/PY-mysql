        },
        {
            "data": {
                "is_heading": 0,
                "text": "BAME women in pregnancy during Covid-19 pandemic/ MSEPO-21003/1.0"
            },
            "type": "ListItem",
            "sequence_num": 64
        },
        {
            "data": {
