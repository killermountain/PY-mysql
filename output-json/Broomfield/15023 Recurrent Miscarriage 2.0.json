        {
            "type": "ListItem",
            "sequence_num": 69,
            "data": {
                "is_heading": 1,
                "text": "Appendix A: Preliminary Equality Analysis"
            }
        },
        {
            "type": "ListItem",
            "sequence_num": 70,
