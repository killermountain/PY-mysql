        {
            "type": "ListItem",
            "sequence_num": 104,
            "data": {
                "is_heading": 0,
                "text": "Follow local guidelines for  Dexamethasone 0.6 mg/kg discharge  iv/po (max 8 mg) single dose Consider dexamethasone  or nebulised budesonide 2 mg 0.15mg/kg po  if po not possible"
            }
        },
        {
            "type": "ListItem",
            "sequence_num": 105,
            "data": {
                "is_heading": 0,
                "text": "Observation for 2-3 hours"
            }
        },
        {
            "type": "ListItem",
            "sequence_num": 106,
            "data": {
                "is_heading": 1,
                "prefix": 2.0,
                "text": "If no improvement or worsening, rescore and act accordingly Stridor  Cyanosis (Osats<92%in air)"
            }
        },
        {
            "type": "ListItem",
            "sequence_num": 107,
            "data": {
                "is_heading": 0,
                "text": "0 = none  0 = none"
            }
        },
        {
            "type": "ListItem",
            "sequence_num": 108,
            "data": {
                "is_heading": 0,
                "text": "1 = at rest, audible with stethoscope  4 = with agitation"
            }
        },
        {
            "type": "ListItem",
            "sequence_num": 109,
            "data": {
                "is_heading": 0,
                "text": "2 = at rest , audible without stethoscope  5 = at rest"
            }
        },
        {
            "type": "ListItem",
            "sequence_num": 110,
            "data": {
                "is_heading": 1,
                "text": "Recession"
            }
        },
        {
            "type": "ListItem",
            "sequence_num": 111,
            "data": {
                "is_heading": 0,
                "text": "0 = none"
            }
        },
        {
            "type": "ListItem",
            "sequence_num": 112,
            "data": {
                "is_heading": 1,
                "text": "1 = mild recession  Level of consciousness 2 = moderate recession  0 = normal"
            }
        },
        {
            "type": "ListItem",
            "sequence_num": 113,
            "data": {
                "is_heading": 1,
                "text": "3 = severe recession  5 = altered mental state"
            }
        },
        {
            "type": "ListItem",
            "sequence_num": 114,
            "data": {
                "is_heading": 0,
                "text": "Croup score 0-1: Mild croup  Croup score 2-7: Moderate croup Croup score >/=8: Severe croup"
            }
        },
